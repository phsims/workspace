{
  "name": "PlanMyPlate",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "dependsOn": ["^build", "build-custom-server"]
    },
    "build-custom-server": {
      "executor": "@nx/js:swc",
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps",
        "main": "apps/server/main.ts",
        "tsConfig": "apps/tsconfig.server.json",
        "clean": false,
        "assets": []
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve-custom-server": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "PlanMyPlate:build-custom-server"
      },
      "configurations": {
        "development": {
          "buildTarget": "PlanMyPlate:build-custom-server:development"
        },
        "production": {
          "buildTarget": "PlanMyPlate:build-custom-server:production"
        }
      }
    }
  }
}
